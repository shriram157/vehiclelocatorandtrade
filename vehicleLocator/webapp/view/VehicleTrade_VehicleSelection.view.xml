<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="vehicleLocator.controller.VehicleTrade_VehicleSelection" xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form"
	xmlns:uxap="sap.uxap" xmlns:t="sap.ui.table" xmlns:u="sap.ui.unified" xmlns:f="sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages class="oImgsampl" height="200px">
			<Page showNavButton="false">
				<customHeader>
					<Bar class="sapUiSmallPaddingBeginEnd clCustomHeaderBg">
						<contentLeft>
							<Image id="idLexusLogo" src="Images/Lexus.png" width="100px"/>
							<Text text="|" class="sapUiTinyMarginBeginEnd"/>
							<Text class="clCustomFont" text="Vehicle Locator-Trade"/>
						</contentLeft>
						<contentRight>
							<Button text="Logout"/>
						</contentRight>
					</Bar>
				</customHeader>
				<content>
					<Toolbar class="toolbar1">
						<ToolbarSpacer></ToolbarSpacer>
						<!--	<Button text="Vehicle Locator" press="handleLinkPress"/>
						<Button text="Sold Order" press="handleLinkPress"/>
						<Button text="Price Protection" press="handleLinkPress"/>
						<Button text="Inventory Summary" press="handleLinkPress"/>
						<Button text="Sujjest Order" press="handleLinkPress"/>-->
					</Toolbar>
					<VBox>
						<OverflowToolbar id="otb1">
							<Label text="{i18n>VehicleTrade-VehicleSelection}" width="310px" class="vHlabe1clss"/>
							<ToolbarSpacer/>
						</OverflowToolbar>
						<VBox>
							<HBox>
								<FlexBox width="40px"></FlexBox>
								<FlexBox height="100px" alignItems="Center" justifyContent="Center">
									<Label text="{i18n>Series}"/>
									<FlexBox width="7px"></FlexBox>
									<ComboBox placeholder="" width="140%" showSecondaryValues="true" filterSecondaryValues="true" selectionChange="handleoVt_SeriesChange"
										id="oVt_SeriesCmbo" items="{/}">
										<core:ListItem text="{TCISeries} {TCISeriesDescriptionEN}" key="{TCISeries}" additionalText=""/>
										<!--	<core:ListItem   text="{TCISeriesDescriptionEN}"      key="{TCISeriesDescriptionEN}"  additionalText =""/>-->
									</ComboBox>
									<!--	<SearchField width="215px" id="idVehclSerTrad"/>-->
								</FlexBox>
							</HBox>
							<Table id="table" width="auto" class="sapUiResponsiveMargin" items="{/}" growingThreshold="150">
								<columns>
									<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
										<Text text="{i18n>Select}"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
										<HBox>
											<Text text="{i18n>VehicleTrackingNumber}"/>
										<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="vtnAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true" />
											<core:Icon src="sap-icon://sort-descending" width="" id="vtnDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
										<HBox>
											<Text text="{i18n>Series}"/>
										<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="senAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="senDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<HBox>
											<Text text="{i18n>Model}"/>
										<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="moaAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="moaDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<HBox>
											<Text text="{i18n>Suffix}"/>
										<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="suAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="suDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<HBox>
											<Text text="{i18n>APX}"/>
										<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="ApsAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="ApxsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<HBox>
											<Text text="{i18n>Color}"/>
										<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="coAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="coDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<HBox>
										<Text text="{i18n>Status}"/>
									<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="staAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="stDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>	
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<HBox>
										<Text text="{i18n>ETAFrom}"/>
								<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="etfAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="etsDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>		
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<HBox>
										<Text text="{i18n>ETATo}"/>
									<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="etofAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="etssDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>		
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true" visible="false">
										<HBox>
										<Text text="{i18n>Year}"/>
									<!--	<core:Icon src="sap-icon://sort-ascending" width="" id="eyeAsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="true"/>
											<core:Icon src="sap-icon://sort-descending" width="" id="yessDsIcon" size="1.3em" class="sapUiTinyMarginBegin" visible="false"/>-->
										</HBox>		
									</Column>
								</columns>
								<items>
									<ColumnListItem press="onLineItemPressedtree1" type="Active">
										<cells>
											<HBox class="sapUiMediumMarginBegin">
												<Link id="oSelectId" text="Select" class="sapUiSmallMarginTop link" subtle="true" press="onSelectLink"/>
											</HBox>
											<Text text="{ZZVTN}"/>
											<Text text="{TCISeries}{TCISeriesDescriptionEN}"/>
											<Text text="{Model}-{ENModelDesc}"/>
											<Text text="{Suffix}-{SuffixDescriptionEN}"/>
											<Text text="{ZZAPX}"/>
											<Text text="{ZZEXTCOL}-{MKTG_DESC_EN}"/>
											<Text text="{ZZORDERTYPE}"/>
											<Text text="{ZZADDDATA4}"/>
											<Text text="{PSTSP}"/>
											<Text text="{Year}"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</VBox>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>