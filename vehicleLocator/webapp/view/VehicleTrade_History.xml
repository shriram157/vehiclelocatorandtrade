<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="vehicleLocator.controller.VehicleTrade_History"
	xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:uxap="sap.uxap" xmlns:t="sap.ui.table" xmlns:u="sap.ui.unified"
	xmlns:f="sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages class="oImgsampl" height="200px">
			<Page showNavButton="false" navButtonPress="onBack">
				<customHeader class="CusHeadclas" height="150px">
					<Bar>
						<contentLeft>
							<Image width="150px" decorative="false" densityAware="false" height="50px" src="Images/Lexus.png"></Image>
						</contentLeft>
					</Bar>
				</customHeader>
				<content>
					<Toolbar class="toolbar2">
						<ToolbarSpacer></ToolbarSpacer>
						<Button text="Vehicle Locator" press="handleLinkPress"/>
						<Button text="Sold Order" press="handleLinkPress"/>
						<Button text="Price Protection" press="handleLinkPress"/>
						<Button text="Inventory Summary" press="handleLinkPress"/>
						<Button text="Sujjest Order" press="handleLinkPress"/>
					</Toolbar>
					<VBox>
						<OverflowToolbar id="otb1">
							<Label text="Vehicle Trade History" width="200px" class="vHlabe1clss"/>
							<ToolbarSpacer/>
							<Button id="openMenVTH" class="VLbtn3" text="Export to Excel" type="Accept" press="handlebacksearch" width="138px"/>
						</OverflowToolbar>
						<VBox>
							<Table id="tableVTH" width="auto" class="sapUiResponsiveMargin" items="{/}" growingThreshold="150">
								<columns>
									<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
										<Text text="Request No"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Status"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="From / To"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Dealer"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Model"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Suffix"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="APX"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Exterior Colour"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Accepted"/>
									</Column>
								</columns>
								<items>
									<ColumnListItem press="onLineItemPressedtree1" type="Navigation">
										<cells>
											<ObjectIdentifier title="{Objid}"/>
											<Text text="{}"/>
											<Text text="{}"/>
											<Text text="{}"/>
											<Text text="{}"/>
											<Text text="{}"/>
											<Text text="{}"/>
											<Text text="{}"/>
											<Text text="{}"/>
											<Text text="{}"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</VBox>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>