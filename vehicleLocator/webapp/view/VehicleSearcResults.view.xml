                         	<Text text="{vehicleSearchTableModel>ort01}" wrapping="false"/>
                          	<Text text="{vehicleSearchTableModel>bezei}" wrapping="false"/>
                          
                          	<Text text="{vehicleSearchTableModel>zzvtn}" wrapping="false"/>
                          
                          	 

