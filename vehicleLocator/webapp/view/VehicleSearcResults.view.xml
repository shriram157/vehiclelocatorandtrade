<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:t="sap.ui.table" xmlns:l="sap.ui.layout"
	xmlns:form="sap.ui.layout.form" xmlns:uxap="sap.uxap" xmlns:u="sap.ui.unified" xmlns:f="sap.ui.layout.form"
	xmlns:html="http://www.w3.org/1999/xhtml" controllerName="vehicleLocator.controller.VehicleSearcResults">
	<App>
		<pages class="oImgsampl" height="200px">
			<Page showNavButton="false">
				<customHeader>
					<Bar class="sapUiSmallPaddingBeginEnd clCustomHeaderBg">
						<contentLeft>
							<Image id="idLexusLogo" src="Images/Lexus.png" width="100px"/>
							<Text text="|" class="sapUiTinyMarginBeginEnd"/>
							<Text class="clCustomFont" text="{i18n>PageTitle}"/>
						</contentLeft>
						<contentRight>
							<!--	<Button text="Logout"/>-->
						</contentRight>
					</Bar>
				</customHeader>
				<content>
					<OverflowToolbar class="toolbar1">
						<!--<ToolbarSpacer/>-->
						<Label text="lets put the logged in dealer name" width="auto" class="vHlabe1clss"/>
						<ToolbarSpacer/>
						<Button id="openMenu3" class="VLbtn1" text="{i18n>PullVehicles}" type="Default" press="handlePressPullvehcls" width="auto"/>
						<Button id="openMenu4" class="VLbtn2" text="{i18n>ExporttoVehicles}" type="Default" press="handleExporttohecls" width="auto"/>
						<Button id="openMenu5" class="VLbtn3" text="{i18n>BacktoSerach}" type="Default" press="handlebacksearch" width="auto"/>
					</OverflowToolbar>
					<f:SimpleForm id="SimpleFormDisplay354" editable="false" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3"
						labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
						singleContainerFullSize="false">
						<f:content>
							<l:VerticalLayout width="80%" class="gridWrapper">
								<l:content>
									<l:Grid containerQuery="true" hSpacing="1">
										<html:div class="sapUiSmallMarginTop sapUiSmallMarginBottom">
											<Label xmlns="sap.m" text="{i18n>Status}" labelFor="VLRStatus" design="Bold" vAlign="Middle" displayOnly="false" wrapping="true"></Label>
											<ComboBox xmlns="sap.m" id="VLRStatus" selectionChange="onStatusChange" width="20rem" items="{/}">
												<core:ListItem text="{path:'zz_trading_ind',formatter:'vehicleLocator.Formatter.Formatter.Status'}" key="{zz_trading_ind}"/>
											</ComboBox>
										</html:div>
										<html:div class="sapUiSmallMarginTop sapUiSmallMarginBottom">
											<Label xmlns="sap.m" text="{i18n>Dealer}" labelFor="VLRDealer" design="Bold"></Label>
											<ComboBox xmlns="sap.m" id="VLRDealer" selectionChange="onStatusChange" width="20rem" items="{/}">
												<!--	<core:ListItem text="{VehicleLocatorScdScr>kunnr} - {VehicleLocatorScdScr>name1}" key="{VehicleLocatorScdScr>kunnr}"/>-->
												<core:ListItem text="{path:'kunnr',formatter:'vehicleLocator.Formatter.Formatter.Dealer'} -{name1}" key="{kunnr}"/>
											</ComboBox>
										</html:div>
										<html:div class="sapUiSmallMarginTop sapUiSmallMarginBottom">
											<Label xmlns="sap.m" text="{i18n>Suffix}" labelFor="VLRSuffix" design="Bold"></Label>
											<ComboBox xmlns="sap.m" id="VLRSuffix" selectionChange="onStatusChange" items="{/}" width="20rem">
												<core:ListItem text="{zzsuffix} - {suffix_desc_en}/{zzintcol} {mrktg_int_desc_en}" key="{zzsuffix}"/>
											</ComboBox>
										</html:div>
										<html:div class="sapUiSmallMarginTop sapUiSmallMarginBottom">
											<Label xmlns="sap.m" text="{i18n>Color}" labelFor="VLRColor" design="Bold"></Label>
											<ComboBox xmlns="sap.m" id="VLRColor" selectionChange="onStatusChange" width="20rem" items="{/}">
												<core:ListItem text="{zzextcol} - {mktg_desc_en}" key="{zzextcol}"/>
											</ComboBox>
										</html:div>
										<CheckBox xmlns="sap.m" text="{i18n>ShowDoNotCall(DNC)Vehicles}" id="chknew" select="onStatusChange"/>
										<CheckBox xmlns="sap.m" text="{i18n>ShowHoldVehicles}" id="chkexi" select="onStatusChange"/>
										<CheckBox xmlns="sap.m" text="{i18n>IncludeSoldVehicles}" id="chkmov" select="onStatusChange" visible="false"/>
										<CheckBox xmlns="sap.m" text="{i18n>IncludeFleetVehicles}" id="chkadd" select="onStatusChange" visible="false"/>
									</l:Grid>
								</l:content>
							</l:VerticalLayout>
						</f:content>
					</f:SimpleForm>
					<!--<HBox>-->
					<!--	<FlexBox width="160px"/>-->
					<!--	<FlexBox height="30px" alignItems="Center" justifyContent="Center">-->
					<!--		<CheckBox text="{i18n>ShowDoNotCall(DNC)Vehicles}" id="chknew" select="onStatusChange"/>-->
					<!--		<FlexBox width="27px"/>-->
					<!--	</FlexBox>-->
					<!--	<FlexBox width="115px"/>-->
					<!--	<FlexBox height="30px" alignItems="Center" justifyContent="Center">-->
					<!--		<CheckBox text="{i18n>ShowHoldVehicles}" id="chkexi" select="onStatusChange"/>-->
					<!--		<FlexBox width="27px"/>-->
					<!--	</FlexBox>-->
					<!--	<FlexBox width="186px"/>-->
					<!--	<FlexBox height="30px" alignItems="Center" justifyContent="Center">-->
					<!--		<CheckBox text="{i18n>IncludeSoldVehicles}" id="chkmov" select="onStatusChange" visible="false"/>-->
					<!--		<FlexBox width="7px"/>-->
					<!--	</FlexBox>-->
					<!--	<FlexBox width="206px"/>-->
					<!--	<FlexBox height="30px" alignItems="Center" justifyContent="Center">-->
					<!--		<CheckBox text="{i18n>IncludeFleetVehicles}" id="chkadd" select="onStatusChange" visible="false"/>-->
					<!--	</FlexBox>-->
					<!--</HBox>-->
					<!--============================================================ new code ===================================================================Begin-->
					<!--<Table id="table1VSR" cellClick="onRowPress" selectionBehavior="Row" selectionMode="None" rows="{/}" visibleRowCount="20">-->
					<!--=================================================================new code ===================================================================End-->
					<t:Table id="table1VSR" cellClick="onRowPress" selectionBehavior="Row" selectionMode="None" rows="{/}" visibleRowCount="20" alternateRowColors="true">
						<t:columns>
							<t:Column id="TradecolId" width="6rem">
								<Label text="{i18n>Trade}"/>
								<t:template>
									<Link id="tradeId" text="Trade" class="link" subtle="true" press="oTradeLinkPress" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="kunnr" width="16rem">
								<Label text="{i18n>Dealer}"/>
								<t:template>
									<Text text="{path:'kunnr',formatter:'vehicleLocator.Formatter.Formatter.Dealer'} -{name1}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="matnr" width="14rem" id="matnr">
								<Label text="{i18n>Model}"/>
								<t:template>
									<Text text="{matnr} - {model_desc_en}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="zzsuffix" width="8rem">
								<Label text="{i18n>Suffix}"/>
								<t:template>
									<Text text="{zzsuffix} - {suffix_desc_en}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="zzapx" width="6rem">
								<Label text="{i18n>APX}" wrapping="false"/>
								<t:template>
									<Text text="{zzapx}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column id="ID_DealerNet" sortProperty="zzextcol" width="14rem">
								<Label text="{i18n>Color}" wrapping="false"/>
								<t:template>
									<Text text="{zzextcol} - {mktg_desc_en}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column id="ID_Profit" sortProperty="zzordertype" width="6rem">
								<Label text="{i18n>OrderType}" wrapping="false"/>
								<t:template>
									<Text text="{path:'zzordertype',formatter:'vehicleLocator.Formatter.Formatter.OrderType'}" wrapping="false"/>
									<!--	<Text text="{zzordertype}" wrapping="false"/>-->
								</t:template>
							</t:Column>
							<t:Column sortProperty="zzadddata4" width="6rem">
								<Label text="{i18n>ETAFrom}" wrapping="false"/>
								<t:template>
									<Text text="{path:'zzadddata4',formatter:'vehicleLocator.Formatter.Formatter.TradeSummaryoDate'}" wrapping="false"/>
									<!--	<Text text="{zzadddata4}" wrapping="false"/>-->
								</t:template>
							</t:Column>
							<t:Column sortProperty="pstsp" width="6rem">
								<Label text="{i18n>ETATo}" wrapping="false"/>
								<t:template>
									<Text text="{path:'pstsp',formatter:'vehicleLocator.Formatter.Formatter.TradeSummaryoDate'}" wrapping="false"/>
									<!--	<Text text="{pstsp}" wrapping="false"/>-->
								</t:template>
							</t:Column>
							<t:Column sortProperty="z_pd_flag" width="6rem">
								<Label text="{i18n>AccessoryInstall}" wrapping="false"/>
								<t:template>
									<Text text="{path:'z_pd_flag',formatter:'vehicleLocator.Formatter.Formatter.AccessoryInstall'}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column width="8rem">
								<Label text="{i18n>City}" wrapping="false"/>
								<t:template>
									<Text text="{ort01}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column width="12rem">
								<Label text="{i18n>Province}" wrapping="false"/>
								<t:template>
									<Text text="{bezei}" wrapping="false"/>
								</t:template>
							</t:Column>
						</t:columns>
					</t:Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>