                        <Text text="{i18n>Suffix}"/>
                    </Column>
                    <Column id="zzapx1"   demandPopin="true"
                            minScreenWidth="Tablet" width="5rem">
                        <Text text="{i18n>APX}" id="nameColumnTitle1"/>
                    </Column>
                    <Column id="ID_DealerNet1"  demandPopin="true"  
