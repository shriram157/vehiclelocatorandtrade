						<Button text="Sujjest Order" press="handleLinkPress"/>-->
					</Toolbar>
					<VBox class="sapUiSmallMargin">
						<OverflowToolbar id="otb1">
							<Label text="{i18n>TradeSummary}" width="200px" class="vHlabe1clss"/>
							<ToolbarSpacer/>
						</OverflowToolbar>
					</VBox>
					<HBox>
						<FlexBox width="40px"></FlexBox>
						<FlexBox height="35px" alignItems="Center" justifyContent="Center">
							<Label text="{i18n>MyTradeRequests}" class="flexdontclabllist"/>
						</FlexBox>
					</HBox>
					<VBox>
						<VBox>
							<t:Table id="tablMyTReq" cellClick="onRowPress" selectionBehavior="Row" selectionMode="None" rows="{/}" visibleRowCount="20">
								<t:columns>
									<t:Column sortProperty="Trade_Id" width="6rem" id="oTradId">
										<Label text="{i18n>Request}"/>
										<t:template>
											<Link id="trade1Id" text="{Trade_Id}" class="link" subtle="true" press="oMyTradeReqLink" wrapping="false"/>
										</t:template>
									</t:Column>
									<t:Column sortProperty="Created_On" width="16rem" id="oCreMTR">
										<Label text="{i18n>Created}"/>
										<t:template>
											<Text text="{Created_On}" wrapping="false"/>
										</t:template>
									</t:Column>
									<t:Column sortProperty="Requested_Dealer" width="14rem" id="reqdealer">
										<Label text="{i18n>Dealer}"/>
										<t:template>
											<Text text="{Requested_Dealer_Name}" wrapping="false"/>
										</t:template>
									</t:Column>
									<t:Column sortProperty="Model" width="8rem" id="oModlMTR">
										<Label text="{i18n>Model}"/>
										<t:template>
											<Text text="{Model}-{Model_Desc}" wrapping="false"/>
										</t:template>
									</t:Column>
									<t:Column sortProperty="SuffID" width="6rem" id="oSuffMTR">
										<Label text="{i18n>Suffix}" wrapping="false"/>
										<t:template>
											<Text text="{Suffix}-{Int_Colour_Desc}" wrapping="false"/>
										</t:template>
									</t:Column>
									<t:Column id="Trade_Return" sortProperty="zzextcol" width="14rem" id="oTrdret">
										<Label text="{i18n>TradeinReturn}" wrapping="false"/>
										<t:template>
											<Text text="{Trade_Return}" wrapping="false"/>
										</t:template>
									</t:Column>
									<t:Column id="Trade_Status" sortProperty="zzordertype" width="6rem" id="oTradereq">
										<Label text="{i18n>Status}" wrapping="false"/>
										<t:template>
											<Text text="{Trade_Status}" wrapping="false"/>
										</t:template>
									</t:Column>
									<t:Column sortProperty="Changed_On" width="6rem" id="oChangedOnMtr">
										<Label text="{i18n>StatusUpdatedDate}" wrapping="false"/>
										<t:template>
											<Text text="{Changed_On}" wrapping="false"/>
										</t:template>
									</t:Column>
								</t:columns>
							</t:Table>
