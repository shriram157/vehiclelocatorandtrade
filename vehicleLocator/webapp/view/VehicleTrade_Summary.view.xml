					</VBox>
					<HBox>
						<FlexBox width="40px"></FlexBox>
						<FlexBox height="25px" alignItems="Center" justifyContent="Center">
							<Label text="{i18n>ReceivedTradeRequests}" class="flexdontclabllist"/>
						</FlexBox>
					</HBox>
					<t:Table id="tablReTReq" cellClick="onRowPress" selectionBehavior="Row" selectionMode="None" rows="{/}" visibleRowCount="20">
						<t:columns>
							<t:Column id="RTradecolId" width="6rem" id="oRtrdcol">
								<Label text="{i18n>Requested}"/>
								<t:template>
									<Link id="trade2Id" text="{Trade_Id}" class="link" subtle="true" press="oMyRecReqLink" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="Requesting_Dealer" width="16rem" id="pReqdealr">
								<Label text="{i18n>FromDealer}"/>
								<t:template>
									<Text text="{path:'Requesting_Dealer',formatter:'vehicleLocator.Formatter.Formatter.'}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="Model" width="14rem" id="matnr2">
								<Label text="{i18n>Model}"/>
								<t:template>
									<Text text="{Model} - {Model_Desc}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="Suffix_Desc" width="8rem" id="oSuffReRq">
								<Label text="{i18n>Suffix}"/>
								<t:template>
									<Text text="{Suffix_Desc} - {Int_Colour_Desc}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="Trade_Return" width="6rem" id="oTradre">
								<Label text="{i18n>TradeinReturn}" wrapping="false"/>
								<t:template>
									<Text text="{Trade_Return}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column id="ID_Status" sortProperty="Trade_Status" width="14rem">
								<Label text="{i18n>Status}" wrapping="false"/>
								<t:template>
