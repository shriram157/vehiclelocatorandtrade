								<Text text="" id="textlab">
									<layoutData>
										<l:GridData span="L4 M12 S3"/>
									</layoutData>
								</Text>
								<Text text="To" id="tolabid"/>
								<Text text="" id="totxtid"/>
								<Label text="Propsed ETA to New Dealer" id="prpdlabid"/>
								<Text text="" id="prpid">
									<layoutData>
										<l:GridData span="L4 M12 S3"/>
									</layoutData>
								</Text>
								<Text text="From" id="fromlabid"/>
								<Label text="" id="frmlabid"></Label>
								<Text text="">
									<layoutData>
										<l:GridData span="L4 M12 S3"/>
									</layoutData>
								</Text>
