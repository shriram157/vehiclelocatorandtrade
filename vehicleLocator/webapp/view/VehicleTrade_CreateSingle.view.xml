								<Button text="{i18n>SelectVehicle}" id="oSeleBtn" press="onSelecveh">
									<layoutData>
										<l:GridData span="L3 M12 S3"/>
									</layoutData>
								</Button>
								<Label text="{i18n>TradeRequestStatus}"/>
