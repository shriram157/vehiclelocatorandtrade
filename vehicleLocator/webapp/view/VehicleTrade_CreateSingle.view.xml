								<Text text="" id="labetxteid" visible="false">
									<layoutData>
										<l:GridData span="L4 M12 S3"/>
									</layoutData>
								</Text>
								<Text text="To" id="ototid" visible="false"/>
								<Text text="" id="otitxt" visible="false"/>
								<Label text="Propsed ETA to New Dealer" id="propetid" visible="false"/>
