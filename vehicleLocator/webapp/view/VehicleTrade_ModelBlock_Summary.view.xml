					<t:Table id="tableVTMBS" cellClick="onRowPress" selectionBehavior="Row" selectionMode="MultiToggle" rows="{/}">
						<t:columns>
						
						
							<t:Column sortProperty="ZzblockedDlr" width="16rem" id="ZzblockedDlr">
								<Label text="{i18n>Dealer}"/>
								<t:template>
									<Text
										text="{ZzblockedDlr}-{OrganizationBPName1}"
										wrapping="false"/>
								</t:template>
							</t:Column>
								<t:Column sortProperty="Zzseries" width="22rem" id="oSeires">
								<Label text="{i18n>Series}" wrapping="false"/>
								<t:template>
									<Text
										text="{Zzseries}-{TCISeriesDescriptionEN}"
										wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="Zzmodel" width="22rem" id="omodelvsr">
								<Label text="{i18n>Model}"/>
								<t:template>
									<Text text="{Zzmodel}-{ModelDescriptionEN}" wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="Zzduration" width="22rem" id="oBlockduration">
								<Label text="{i18n>BlockDuration}" wrapping="false"/>
								<t:template>
									<Text
										text="{Zzduration}"
										wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="ZzblkstartDate" width="14rem" id="fromdate">
								<Label text="{i18n>FromDate}" wrapping="false"/>
								<t:template>
									<Text text="{path:'ZzblkstartDate',formatter:'vehicleLocator.Formatter.Formatter.TradeSummaryoDate'}"
										wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="ZzblkendDate" width="6rem" id="todate">
								<Label text="{i18n>ToDate}" wrapping="false"/>
								<t:template>
									<Text text="{path:'ZzblkendDate',formatter:'vehicleLocator.Formatter.Formatter.TradeSummaryoDate'}"
										wrapping="false"/>
								</t:template>
							</t:Column>
							<t:Column sortProperty="Zzcomment" width="14rem" id="comment">
								<Label text="{i18n>Comments}" wrapping="false"/>
								<t:template>
									<Text text="{Zzcomment}"
										wrapping="false"/>
								</t:template>
							</t:Column>
						</t:columns>
