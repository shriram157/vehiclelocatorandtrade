									<core:ListItem key="{BusinessPartner}" text="{path:'BusinessPartner',formatter:'vehicleLocator.Formatter.Formatter.TradeSummaryoDealer'}" additionalText="{OrganizationBPName1}"/>
									
								</ComboBox>
								<!--	<SearchField id="Searcid" width="76%"/>-->
