{

	"welcomeFile": "/vehicleLocator/index.html",
	"authenticationMethod": "none",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/vehicleLocator/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "/node(.*)",
		"destination": "vehicleLocator-node-backend",
		"csrfProtection": false,
		"authenticationType": "none"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "vehicleLocator_XSJS_backend",
		"authenticationType": "none"
    }, {
		"source": "/xsodata/(.*)",
		"destination": "vehicleLocator_XSJS_backend",
		"authenticationType": "none"
	}]
}

