{

	"welcomeFile": "/vehicleLocator/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/vehicleLocator/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "/node(.*)",
		"destination": "vehicleLocator_Node_backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsjs)",
		"destination": "vehicleLocator_XSJS_backend",
		"csrfProtection": false,
		"authenticationType": "none"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "vehicleLocator_XSJS_backend",
		"authenticationType": "none"
    }, {
		"source": "/xsodata/(.*)",
		"destination": "vehicleLocator_XSJS_backend",
		"authenticationType": "none"
	}, {
			"source": "/userDetails(.*)",
			"destination": "vehicleLocator_Node_backend",
			"csrfProtection": false,
			"authenticationType": "xsuaa"

		}
	]
}

